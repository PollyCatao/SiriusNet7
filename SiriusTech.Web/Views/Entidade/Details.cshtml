@model SiriusTech.Data.Models.Entidade

@{
    ViewData["Title"] = "Detalhes do Entidade";
}

<h1>Detalhes do Entidade</h1>

<div>
    <h4>Confira os detalhes abaixo</h4>
    <hr />
    <div>
        <a asp-action="Edit" class="btn btn-success" asp-route-id="@Model?.IdEntidade">Alterar</a> 
        <a asp-action="Index" class="btn btn-primary">Voltar</a>
    </div>
    <br />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CpfCnpj)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CpfCnpj)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DcrEntidade)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DcrEntidade)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DcrEndereco)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DcrEndereco)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdCidade)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdCidade)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdBairro)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdBairro)
        </dd>
    </dl>
</div>
